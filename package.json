{
  "name": "huayen-front-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "axios-vue": "^0.1.3",
    "core-js": "^3.6.5",
    "node-sass": "6",
    "vue": "^3.0.0",
    "vue-axios": "^3.4.1",
    "vue-loading-overlay": "^5.0",
    "vue-mobile-detection": "^2.0.1",
    "vue-router": "4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.9",
    "@vue/cli-plugin-eslint": "~4.5.9",
    "@vue/cli-service": "~4.5.9",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0",
    "sass-loader": "10",
    "style-resources-loader": "^1.4.1",
    "vue-cli-plugin-style-resources-loader": "~0.1.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
        "indent": ["warn", 4],
        "no-empty": "warn",
        "no-unused-vars": "warn",
        "linebreak-style": ["warn", "unix"],
        "quotes": ["warn", "single", { "avoidEscape": true, "allowTemplateLiterals": true }]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
